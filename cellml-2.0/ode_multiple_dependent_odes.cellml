<model xmlns="http://www.cellml.org/cellml/2.0#" name="my_model">
    <!-- Multiple dependent ODEs (this is actually the van der Pol 1928 model)
   d(x)/d(t) = y
   d(y)/d(t) = (epsilon*(1-x^2)*y-x)
   x(0) = -2
   y(0) = 0
   epsilon = 1-->
    <units name="per_s">
        <unit exponent="-1" units="second"/>
    </units>
    <component name="environment">
        <variable name="t" units="second" interface="public"/>
    </component>
    <component name="my_x_ode">
        <variable name="t" units="second" interface="public"/>
        <variable initial_value="-2" name="x" units="dimensionless" interface="public"/>
        <variable name="y" units="dimensionless" interface="public"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>t</ci>
                    </bvar>
                    <ci>x</ci>
                </apply>
                <apply>
                    <times/>
                    <ci>y</ci>
                    <cn units="per_s">1</cn>
                </apply>
            </apply>
        </math>
    </component>
    <component name="my_y_ode">
        <variable name="t" units="second" interface="public"/>
        <variable name="x" units="dimensionless" interface="public"/>
        <variable initial_value="0" name="y" units="dimensionless" interface="public"/>
        <variable initial_value="1" name="epsilon" units="dimensionless"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>t</ci>
                    </bvar>
                    <ci>y</ci>
                </apply>
                <apply>
                    <times/>
                    <apply>
                        <minus/>
                        <apply>
                            <times/>
                            <ci>epsilon</ci>
                            <apply>
                                <minus/>
                                <cn units="dimensionless">1</cn>
                                <apply>
                                    <power/>
                                    <ci>x</ci>
                                    <cn units="dimensionless">2</cn>
                                </apply>
                            </apply>
                            <ci>y</ci>
                        </apply>
                        <ci>x</ci>
                    </apply>
                    <cn units="per_s">1</cn>
                </apply>
            </apply>
        </math>
    </component>
    <connection component_1="my_x_ode" component_2="environment">
        <map_variables variable_1="t" variable_2="t"/>
    </connection>
    <connection component_1="my_y_ode" component_2="environment">
        <map_variables variable_1="t" variable_2="t"/>
    </connection>
    <connection component_1="my_x_ode" component_2="my_y_ode">
        <map_variables variable_1="x" variable_2="x"/>
        <map_variables variable_1="y" variable_2="y"/>
    </connection>
</model>

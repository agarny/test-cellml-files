<model xmlns="http://www.cellml.org/cellml/2.0#" name="my_model">
    <!-- Pseudo DAE v1
   d(x)/d(t) = cos(y)
   x(0) = 1
   sin(x)-y = 0
   Note: both v1 and v2 ought to generate the exact same code...-->
    <units name="per_s">
        <unit exponent="-1" units="second"/>
    </units>
    <component name="environment">
        <variable name="t" units="second" interface="public"/>
    </component>
    <component name="my_dae">
        <variable name="t" units="second" interface="public"/>
        <variable initial_value="1" name="x" units="dimensionless"/>
        <variable name="y" units="dimensionless"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>t</ci>
                    </bvar>
                    <ci>x</ci>
                </apply>
                <apply>
                    <times/>
                    <apply>
                        <cos/>
                        <ci>y</ci>
                    </apply>
                    <cn units="per_s">1</cn>
                </apply>
            </apply>
            <apply>
                <eq/>
                <apply>
                    <minus/>
                    <apply>
                        <sin/>
                        <ci>x</ci>
                    </apply>
                    <ci>y</ci>
                </apply>
                <cn units="dimensionless">0</cn>
            </apply>
        </math>
    </component>
    <connection component_1="my_dae" component_2="environment">
        <map_variables variable_1="t" variable_2="t"/>
    </connection>
</model>

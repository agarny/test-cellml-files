<model xmlns="http://www.cellml.org/cellml/2.0#" name="my_model">
    <!-- DAE with NLA system to be solved only once
   d(x)/d(t) = 1
   x(0) = 0
   a^2 = 9-->
    <units name="per_s">
        <unit exponent="-1" units="second"/>
    </units>
    <component name="my_component">
        <variable name="t" units="second"/>
        <variable initial_value="0" name="x" units="dimensionless"/>
        <variable name="a" units="dimensionless"/>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <apply>
                    <diff/>
                    <bvar>
                        <ci>t</ci>
                    </bvar>
                    <ci>x</ci>
                </apply>
                <cn units="per_s">1</cn>
            </apply>
        </math>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
                <eq/>
                <apply>
                    <power/>
                    <ci>a</ci>
                    <cn units="dimensionless">2</cn>
                </apply>
                <cn units="dimensionless">9</cn>
            </apply>
        </math>
    </component>
</model>
